{
  "expo": {
    "name": "MyTrip",
    "slug": "mytrip",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.6",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.velatristudio.mytrip"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.velatristudio.mytrip"
    }
  }
}

